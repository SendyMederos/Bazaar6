    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.11.8",
